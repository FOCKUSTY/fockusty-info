{
  "name": "fockusty",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "pnpm run eslint && react-scripts start",
    "build": "pnpm run eslint && react-scripts build",
    "eslint": "pnpm dlx eslint src/**",
    "test": "react-scripts test",
    "format": "prettier --write \"./**.ts\" \"./**.tsx\" \"./**.css\" \"./**.json\"",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-icons": "^5.3.0",
    "react-router-dom": "^6.27.0",
    "react-scripts": "5.0.1",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.113",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.37.3",
    "globals": "^15.14.0",
    "prettier": "^3.3.3",
    "typescript": "^4.9.5",
    "typescript-eslint": "^8.19.1"
  }
}
